{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "bundler",
    "experimentalDecorators": true,
    "importHelpers": true,
    "strict": true,
    "target": "ES2022",
    "skipLibCheck": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2022",
      "dom"
    ],
    "paths": {
      "@soulonfire/ng-dynamic-forms-core": [
        "dist/ng-dynamic-forms/core"
      ],
      "@soulonfire/ng-dynamic-forms-core/*": [
        "dist/ng-dynamic-forms/core/*"
      ],
      "@soulonfire/ng-dynamic-forms-ui-basic": [
        "dist/ng-dynamic-forms/ui-basic"
      ],
      "@soulonfire/ng-dynamic-forms-ui-basic/*": [
        "dist/ng-dynamic-forms/ui-basic/*"
      ],
      "@soulonfire/ng-dynamic-forms-ui-bootstrap": [
        "dist/ng-dynamic-forms/ui-bootstrap"
      ],
      "@soulonfire/ng-dynamic-forms-ui-bootstrap/*": [
        "dist/ng-dynamic-forms/ui-bootstrap/*"
      ],
      "@soulonfire/ng-dynamic-forms-ui-foundation": [
        "dist/ng-dynamic-forms/ui-foundation"
      ],
      "@soulonfire/ng-dynamic-forms-ui-foundation/*": [
        "dist/ng-dynamic-forms/ui-foundation/*"
      ],
      "@soulonfire/ng-dynamic-forms-ui-ionic": [
        "dist/ng-dynamic-forms/ui-ionic"
      ],
      "@soulonfire/ng-dynamic-forms-ui-ionic/*": [
        "dist/ng-dynamic-forms/ui-ionic/*"
      ],
      "@soulonfire/ng-dynamic-forms-ui-material": [
        "dist/ng-dynamic-forms/ui-material"
      ],
      "@soulonfire/ng-dynamic-forms-ui-material/*": [
        "dist/ng-dynamic-forms/ui-material/*"
      ],
      "@soulonfire/ng-dynamic-forms-ui-ng-bootstrap": [
        "dist/ng-dynamic-forms/ui-ng-bootstrap"
      ],
      "@soulonfire/ng-dynamic-forms-ui-ng-bootstrap/*": [
        "dist/ng-dynamic-forms/ui-ng-bootstrap/*"
      ],
      "@soulonfire/ng-dynamic-forms-ui-primeng": [
        "dist/ng-dynamic-forms/ui-primeng"
      ],
      "@soulonfire/ng-dynamic-forms-ui-primeng/*": [
        "dist/ng-dynamic-forms/ui-primeng/*"
      ],
      "@soulonfire/ng-dynamic-forms-ui-ngx-bootstrap": [
        "dist/ng-dynamic-forms/ui-ngx-bootstrap"
      ],
      "@soulonfire/ng-dynamic-forms-ui-ngx-bootstrap/*": [
        "dist/ng-dynamic-forms/ui-ngx-bootstrap/*"
      ]
    },
    "useDefineForClassFields": false
  }
}
